{
  "name": "almond-re",
  "version": "1.0.0",
  "description": "Almond frontend application for the hydroponics farm",
  "main": "index.js",
  "scripts": {
    "start:dev": "webpack serve --config config/webpack.dev.js --host froyo.almond.com",
    "start:prod": "yarn run build && node server.js",
    "start": "node server.js",
    "start:heroku": "node --optimize_for_size --max_old_space_size=920 --gc_interval=100 server.js",
    "build": "webpack --config config/webpack.prod.js --mode production",
    "eslint": "eslint .",
    "eslint:fix": "eslint \"./src/**/*.{ts,tsx}\" --fix",
    "lint:scss": "stylelint --fix",
    "lint:ts": "eslint --ext=jsx,ts,tsx src",
    "tslint": "tsc --noEmit && eslint \"*/**/*.{js,ts,tsx}\" --quiet",
    "stylelint": "stylelint '**/*.*(css|scss)'",
    "stylelint:fix": "yarn stylelint --fix",
    "test:unit": "jest -u --maxWorkers=4",
    "test:coverage": "yarn test:unit --coverage",
    "test:watch": "yarn test:unit --watch --verbose",
    "cy:open": "cypress open",
    "cy:run": "cypress run --browser chrome",
    "cy:open:dev": "server-test start:dev 3000 cy:open",
    "cy:e2e:prod": "yarn run cy:run",
    "analyze": "webpack --profile --json > stats.json",
    "workbox:inject": "workbox injectManifest workbox-config.js",
    "docker:build": "docker build -t almond-web .",
    "docker:run": "docker run --env-file=.env -p 3000:80 almond-web",
    "husky": "pretty-quick --staged && yarn prettier && yarn tslint",
    "prettier-watch": "onchange \"**/*\" -- prettier --write --ignore-unknown {{changed}}"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mashafrancis/almond-re.git"
  },
  "author": "Francis Masha <francismasha96@gmail.com>",
